Mulot
-- Cette procedure dessine
-- un carre dont on donne 
-- la taille en parametre
Proc Carre (Mulot t, int taille)
Pour i = 0 a 3
	t.avance(taille);
	t.tourne(90);
Ruop
Corp
taille = 480;
monMulot = new Mulot(80, 480, RED);
monMulot.baisse();
Pour i  = 0 a 8
	Carre(monMulot, taille);
	monMulot.avance(taille / 2);
	monMulot.tourne(45);
	taille = taille*141/200;
Ruop
monMulot.leve();
monMulot.avance(100);
Tolum
